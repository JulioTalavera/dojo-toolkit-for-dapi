- if (module.properties)
    h2.jsdoc-summary-heading Property Summary
        span.jsdoc-summary-toggle
    div.jsdoc-summary-list
        ul.jsdoc-property-list
            each prop in module.properties
                li.todoIcon
                    a.inline-link(href='##{prop.name}')  !{prop.name}  -  !{prop.scope}  -  !{prop.type}  -   !{prop.from} 
                    span !{prop.shortSummary}
                    // span !{prop.summary}
